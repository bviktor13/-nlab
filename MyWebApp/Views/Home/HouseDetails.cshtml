@model MyWebApp.Models.HomeViewModels.HouseDetailsView
@{
    ViewBag.Title = "HousesDetails";

}

<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="HouseDetails.Price"></label>
                <input asp-for="HouseDetails.Price" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="City"></label>
                <input asp-for="City.Name" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="HouseDetails.Street"></label>
                <input asp-for="HouseDetails.Street" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="HouseDetails.HouseNumber"></label>
                <input asp-for="HouseDetails.HouseNumber" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="HouseDetails.RoomNumber"></label>
                <input asp-for="HouseDetails.RoomNumber" class="form-control" />
            </div>5
            <div class="form-group">
                <label asp-for="HouseDetails.PartyRoomNumber"></label>
                <input asp-for="HouseDetails.PartyRoomNumber" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="HouseDetails.Animal"></label>
                <input asp-for="HouseDetails.Animal" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="HouseDetails.HouseType"></label>
                <input asp-for="HouseDetails.HouseType" class="form-control" />
            </div>
        </form>
    </div>


@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
