@model MyWebAppDal.Model.House
@{
    ViewBag.Title = "HousesDetails";

}

<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Price"></label>
                <input asp-for="Price" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="City"></label>
                <input asp-for="City" class="form-control" />
             </div>
                <div class="form-group">
                    <label asp-for="Street"></label>
                    <input asp-for="Street" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="HouseNumber"></label>
                    <input asp-for="HouseNumber" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="RoomNumber"></label>
                    <input asp-for="RoomNumber" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="PartyRoomNumber"></label>
                    <input asp-for="PartyRoomNumber" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="Animal"></label>
                    <input asp-for="Animal" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="HouseType"></label>
                    <input asp-for="HouseType" class="form-control" />
                </div>
        </form>
    </div>
</div>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
